pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', url: 'https://github.com/Mayuresh4398/TE7.git'
            }
        }
        stage('compile') {
            steps {
                bat'javac helloworld.java'
            }
        }
        stage('run') {
            steps {
                 bat'java helloworld'
            }
        }
    }
}
